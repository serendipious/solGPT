    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "^0.73.9",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "3.2.4",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
