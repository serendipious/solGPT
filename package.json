    "@sentry/react-native": "^4.8.0",
    "axios": "^0.25.0",
    "chance": "^1.1.9",
    "clsx": "^1.2.1",
    "expr-eval": "^2.0.2",
    "fuse.js": "^6.5.3",
    "immer": "^9.0.12",
    "intl": "^1.2.5",
    "lodash": "^4.17.21",
    "luxon": "^2.3.0",
    "mobx": "^6.3.12",
    "mobx-react-lite": "3.4.0",
    "nanoid": "^4.0.0",
    "nativewind": "2.0.11",
    "patch-package": "^6.5.1",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.2.0",
    "react-native": "0.71.6",
