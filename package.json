    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/axios": "^0.14.0",
    "@types/chance": "^1.1.3",
    "@types/lodash": "^4.14.182",
    "@types/luxon": "^2.0.9",
    "@types/react-native": "^0.66.12",
    "@types/uuid": "^9.0.0",
    "babel-jest": "^29.2.1",
